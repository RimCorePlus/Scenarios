<Defs>
    <PawnKindDef Abstract="True" Name="RCP_BasePlayerPawnKind">
        <apparelIgnorePollution MayRequire="Ludeon.RimWorld.Biotech">true</apparelIgnorePollution>
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <biocodeWeaponChance>1</biocodeWeaponChance>
        <combatPower>30</combatPower>
        <defaultFactionType>PlayerColony</defaultFactionType>
        <humanPregnancyChance>0</humanPregnancyChance>
        <initialResistanceRange>13~21</initialResistanceRange>
        <initialWillRange>2~4</initialWillRange>
        <isFighter>false</isFighter>
        <race>Human</race>
        <techHediffsDisallowTags>
            <li>PainCauser</li>
        </techHediffsDisallowTags>
    </PawnKindDef>
    <PawnKindDef ParentName="RCP_BasePlayerPawnKind">
        <defName>RCP_PlayerMercenary</defName>
        <label>mercenary</label>
        <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
        <apparelDisallowTags>
            <li>RCP_GasMaskExclusion</li>
        </apparelDisallowTags>
        <apparelMoney>1000~1500</apparelMoney>
        <apparelTags>
            <li>IndustrialBasic</li>
            <li>IndustrialAdvanced</li>
            <li>IndustrialMilitaryBasic</li>
            <li>IndustrialMilitaryAdvanced</li>
        </apparelTags>
        <combatPower Inherit="False">85</combatPower>
        <disallowedTraits>
            <li>Brawler</li>
        </disallowedTraits>
        <minGenerationAge>20</minGenerationAge>
        <skills>
            <li>
                <skill>Shooting</skill>
                <range>5~15</range>
            </li>
        </skills>
        <techHediffsChance>0.15</techHediffsChance>
        <techHediffsMoney>700~1200</techHediffsMoney>
        <techHediffsTags>
            <li>Simple</li>
            <li>Advanced</li>
            <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <weaponMoney>355~650</weaponMoney>
        <weaponTags>
            <li>IndustrialGunAdvanced</li>
        </weaponTags>
    </PawnKindDef>
</Defs>