<Defs>
    <!--base-->
    <PawnKindDef Abstract="True" Name="CorePlus_BasePlayerPawnKind">
        <race>Human</race>
        <combatPower>30</combatPower>
        <isFighter>false</isFighter>
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <apparelIgnorePollution MayRequire="Ludeon.RimWorld.Biotech">true</apparelIgnorePollution>
        <initialWillRange>0~0</initialWillRange>
        <humanPregnancyChance>0</humanPregnancyChance>
        <defaultFactionType>PlayerColony</defaultFactionType>
    </PawnKindDef>
    <!--mechanitor in black-->
    <PawnKindDef ParentName="CorePlus_BasePlayerPawnKind">
        <defName>CorePlus_MechanitorInBlack</defName>
        <label>mechanitor in black</label>
        <labelPlural>mechanitors in black</labelPlural>
        <combatPower>55</combatPower>
        <minGenerationAge>20</minGenerationAge>
        <maxGenerationAge>50</maxGenerationAge>
        <chemicalAddictionChance>0.05</chemicalAddictionChance>
        <backstoryCryptosleepCommonality>0.05</backstoryCryptosleepCommonality>
        <backstoryFiltersOverride>
            <li>
                <categories>
                    <li>Offworld</li>
                    <li>Outlander</li>
                    <li>Outsider</li>
                </categories>
            </li>
        </backstoryFiltersOverride>
        <requiredWorkTags>
            <li>Violent</li>
            <li>Caring</li>
            <li>Intellectual</li>
        </requiredWorkTags>
        <gearHealthRange>1~1</gearHealthRange>
        <itemQuality>Excellent</itemQuality>
        <minApparelQuality>Excellent</minApparelQuality>
        <forceWeaponQuality>Excellent</forceWeaponQuality>
        <weaponMoney>500~1000</weaponMoney>
        <weaponTags>
            <li>Revolver</li>
            <li IfModActive="harkon.other.coltanaconda">HRK_Colt_Anaconda_M</li>
        </weaponTags>
        <techHediffsRequired>
            <li>Mechlink</li>
        </techHediffsRequired>
        <biocodeWeaponChance>1</biocodeWeaponChance>
        <apparelTags>
            <li>IndustrialBasic</li>
            <li>IndustrialAdvanced</li>
        </apparelTags>
        <apparelRequired>
            <li>Apparel_CollarShirt</li>
            <li>Apparel_Pants</li>
            <li>Apparel_Duster</li>
            <li>Apparel_FlakVest</li>
            <li>Apparel_CowboyHat</li>
            <li MayRequire="vanillaexpanded.vappe">VAE_Footwear_Boots</li>
        </apparelRequired>
        <apparelColor>(58,58,58)</apparelColor>
        <ignoreIdeoApparelColors>true</ignoreIdeoApparelColors>
        <favoriteColor>(58,58,58)</favoriteColor>
        <initialWillRange>0~0</initialWillRange>
        <initialResistanceRange>0~2</initialResistanceRange>
        <skills>
            <li>
                <skill>Shooting</skill>
                <range>8~16</range>
            </li>
        </skills>
        <disallowedTraits>
            <li>BodyPurist</li>
        </disallowedTraits>
        <disallowedTraitsWithDegree>
            <PsychicSensitivity>-1</PsychicSensitivity>
            <PsychicSensitivity>-2</PsychicSensitivity>
        </disallowedTraitsWithDegree>
    </PawnKindDef>
    <!--highmate refugee-->
    <PawnKindDef ParentName="CorePlus_BasePlayerPawnKind">
        <defName>CorePlus_HighmateRefugee</defName>
        <label>highmate refugee</label>
        <minGenerationAge>16</minGenerationAge>
        <maxGenerationAge>22</maxGenerationAge>
        <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
        <requiredWorkTags>
            <li>Social</li>
        </requiredWorkTags>
        <gearHealthRange>1~1</gearHealthRange>
        <itemQuality>Normal</itemQuality>
        <minApparelQuality>Normal</minApparelQuality>
        <forceWeaponQuality>Normal</forceWeaponQuality>
        <apparelTags>
            <li>IndustrialBasic</li>
            <li>IndustrialAdvanced</li>
        </apparelTags>
        <apparelRequired>
            <li>Apparel_CollarShirt</li>
            <li>Apparel_Pants</li>
            <li>Apparel_Jacket</li>
            <li MayRequire="vanillaexpanded.vappe">VAE_Footwear_Shoes</li>
        </apparelRequired>
        <apparelColor>(232,232,232)</apparelColor>
        <ignoreIdeoApparelColors>true</ignoreIdeoApparelColors>
        <techHediffsMoney>50~800</techHediffsMoney>
        <techHediffsTags>
            <li>Advanced</li>
        </techHediffsTags>
        <techHediffsChance>0.04</techHediffsChance>
        <xenotypeSet>
            <xenotypeChances>
                <Highmate MayRequire="Ludeon.RimWorld.Biotech">999</Highmate>
            </xenotypeChances>
        </xenotypeSet>
        <initialResistanceRange>0~2</initialResistanceRange>
        <skills>
            <li>
                <skill>Social</skill>
                <range>8~16</range>
            </li>
        </skills>
        <fixedGender>Female</fixedGender>
        <disallowedTraits>
            <li>Gay</li>
        </disallowedTraits>
    </PawnKindDef>
    <!--mercenary sniper-->
    <PawnKindDef ParentName="CorePlus_BasePlayerPawnKind">
        <defName>CorePlus_MercSniper</defName>
        <label>sniper</label>
        <defaultFactionType>PlayerColony</defaultFactionType>
        <minGenerationAge>25</minGenerationAge>
        <maxGenerationAge>40</maxGenerationAge>
        <combatPower>110</combatPower>
        <gearHealthRange>1~1</gearHealthRange>
        <itemQuality>Normal</itemQuality>
        <minApparelQuality>Normal</minApparelQuality>
        <forceWeaponQuality>Normal</forceWeaponQuality>
        <apparelTags>
            <li>IndustrialBasic</li>
            <li>IndustrialAdvanced</li>
            <li>IndustrialMilitaryBasic</li>
            <li>IndustrialMilitaryAdvanced</li>
        </apparelTags>
        <apparelMoney>1000~1500</apparelMoney>
        <apparelColor>(55,55,55)</apparelColor>
        <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
        <weaponMoney>600~600</weaponMoney>
        <weaponTags>
            <li>SniperRifle</li>
        </weaponTags>
        <initialResistanceRange>13~20</initialResistanceRange>
        <skills>
            <li>
                <skill>Shooting</skill>
                <range>8~16</range>
            </li>
        </skills>
        <fixedGender>Male</fixedGender>
        <disallowedTraits>
            <li>Gay</li>
            <li>Brawler</li>
        </disallowedTraits>
        <techHediffsChance>0.15</techHediffsChance>
        <techHediffsMoney>700~1200</techHediffsMoney>
        <techHediffsTags>
            <li>Simple</li>
            <li>Advanced</li>
            <li>ImplantEmpireCommon</li>
        </techHediffsTags>
        <biocodeWeaponChance>1</biocodeWeaponChance>
    </PawnKindDef>
</Defs>